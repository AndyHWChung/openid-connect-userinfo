{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "User Info",
  "type": "object",
  "properties": {
    "given_name": {
      "type": "string",
      "description": "End-user's first name.",
      "example": "Thomas"
    },
    "family_name": {
      "type": "string",
      "description": "End-user's last name.",
      "example": "Delgado"
    },
    "birthdate": {
      "type": "string",
      "description": "End-User's birthday, represented as an ISO 8601:2004 [ISO8601â€‘2004] YYYY-MM-DD format.",
      "example": "1996-08-10"
    },
    "uk_gov_nino": {
      "type": "string",
      "description": "End-User's National Insurance Number.",
      "example": "GG853817C"
    },
    "address": {
      "type": "object",
      "properties": {
        "formatted": {
          "type": "string",
          "description": "End-user's mailing address, formatted for display or use on a mailing label.",
          "example": "221B Baker Street\nLondon\nNW1 9NT\nGreat Britain"
        },
        "postal_code": {
          "type": "string",
          "description": "End-user's Zip code or postal code.",
          "example": "NW1 9NT"
        },
        "country": {
          "type": "string",
          "description": "End-user's country name.",
          "example": "Great Britain"
        }
      }
    }
  }
}
